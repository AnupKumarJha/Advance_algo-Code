double bruteF_min_dist(points pt[],int n){
    double min=DBL_MAX;
    
    for(int i=0;i<n;i++){
        
        
        for(int j=i+1;j<n;j++){
            double d=sqrt(pow((pt[i].x-pt[j].x),2)+pow((pt[i].y-pt[j].y),2));
            if(min>d) min=d;
        }
    }
    return min;
}


